$shm_hitalama_app $mol_book2_catalog
	param \section
	menu_title \Hitalama
	plugins /
		<= Theme $mol_theme_auto
	tools /
		<= Lights $mol_lights_toggle
	menu_body /
		<= Tokens $mol_view
			sub /
				<= Tokens_label $mol_text
					text \Сбор осуществляется от имени
				<= Token_switch $shm_hitalama_token_switch
					current => token
		^
	spreads *
		group <= Group_page $shm_hitalama_group
			token <= token
		lists <= Lists $mol_page
			title \Списки сообществ
